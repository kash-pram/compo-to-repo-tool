{
  "baseFiles": [
    "package.json",
    "angular.json",
    "tsconfig.json",
    "tsconfig.app.json",
    "tsconfig.spec.json",
    ".gitignore",
    "src/main.ts",
    "src/index.html",
    "src/styles.css",
    "src/styles.scss",
    "src/app/app.ts",
    "src/app/app.html",
    "src/app/app.css",
    "src/app/app.config.ts",
    "src/app/app.routes.ts"
  ],
  "githubUsername": "your-github-username",
  "defaultVisibility": "public",
  "alwaysIncludeFolders": [
    "src/app/services",
    "src/app/models",
    "src/app/shared",
    "src/environments"
  ],
  "alwaysIncludeFiles": [
    "src/environments/environment.ts",
    "src/environments/environment.prod.ts"
  ],
  "documentationFiles": [
    "LICENSE",
    "DISCLAIMER.md",
    "docs/CODE_OF_CONDUCT.md",
    "docs/CONTRIBUTING.md",
    "SECURITY.md"
  ],
  "githubPages": {
    "enabled": true,
    "createWorkflow": true,
    "create404": true,
    "verifyBuild": true,
    "configurePermissions": true,
    "retryAttempts": 3
  }
}